{
  "name": "nuxt-tailwind",
  "version": "1.0.0",
  "description": "My luminous Nuxt.js project",
  "author": "Adrien Beaudouin",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "openapi": "openapi-generator generate -g typescript-fetch -o openapi --additional-properties=typescriptThreePlus=true -i"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/recaptcha": "^0.5.3",
    "form-data": "^2.5.1",
    "nuxt": "^2.10.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.3.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/router": "^1.5.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "@openapitools/openapi-generator-cli": "^1.0.1-4.1.3",
    "@tailwindcss/custom-forms": "^0.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "prettier": "^1.16.4",
    "vue-lazyload": "^1.3.3"
  }
}
